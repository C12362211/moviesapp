<h1>Sign in</h1>
<p>Find me in app/views/sessions/new.html.erb</p>
<%= form_tag sessions_path do %>
	<table>
	 <tr>
			<td><%= label_tag :name %></td>
			 <td><%= text_field_tag :name %></td>
	 </tr>
	 <tr>
			<td><%= label_tag :password %></td>
			 <td><%= password_field_tag :password %></td>
	 </tr>
	 <tr>
		<td><%= submit_tag "Sign in" %></td>
		</tr>
	</table>
<% end %>
